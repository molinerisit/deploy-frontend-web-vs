{
  "rewrites": [
    {
      "source": "/((?!assets/|favicon\\.ico|robots\\.txt|manifest\\.webmanifest|.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
